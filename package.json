  "description": "Website Portfolio Profesional Januar Galuh Prabakti",
  "author": "Januar Galuh Prabakti <januargaluh3099@gmail.com>",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:windows": "npm run fix-windows && next dev",
    "dev:turbo": "next dev --turbo",
    "dev:fast": "next dev --turbo --experimental-optimization",
    "dev:legacy": "next dev --experimental-legacy-prebundling",
    "build": "next build",
    "postbuild": "node scripts/optimize-loading.js",
    "start": "next start",
    "lint": "next lint",
    "clean": "if exist .next rd /s /q .next",
    "clean:full": "npm run clean && if exist node_modules\\.cache rd /s /q node_modules\\.cache",
    "refresh-fonts": "node scripts/fix-fonts.js",
    "fix-windows": "npm run clean:full && npm run refresh-fonts",
    "prepare-deploy": "npm run fix-windows && npm run build",
    "deploy:vercel": "vercel",
    "deploy:vercel-prod": "vercel --prod",
    "export": "next build && next export -o out",
    "prepare-gh-pages": "node scripts/prepare-gh-pages.js",
    "deploy:gh-pages": "npm run export && npm run prepare-gh-pages && npx gh-pages -d out",
    "deploy:cloudflare": "npm run build && npx wrangler pages deploy .next",
    "dev:cloudflare": "npx wrangler pages dev .next --port 8788"
  },
