  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:windows": "npm run fix-windows && next dev",
    "dev:turbo": "next dev --turbo",
    "dev:fast": "next dev --turbo --experimental-optimization",
    "dev:legacy": "next dev --experimental-legacy-prebundling",
    "pre-build": "node scripts/pre-build.js && node scripts/direct-patching.js",
    "build": "npm run pre-build && next build",
    "build:gh-pages": "npm run pre-build && cross-env DEPLOY_TARGET=gh-pages next build",
    "postbuild": "node scripts/fix-asset-paths.js",
    "start": "next start",
    "lint": "next lint",
    "clean": "if exist .next rd /s /q .next",
    "clean:full": "npm run clean && if exist node_modules\\.cache rd /s /q node_modules\\.cache",
    "refresh-fonts": "node scripts/fix-fonts.js",
    "fix-windows": "npm run clean:full && npm run refresh-fonts",
    "prepare-deploy": "npm run fix-windows && npm run build",
    "deploy:vercel": "vercel",
    "deploy:vercel-prod": "vercel --prod",
    "export": "next build",
    "export:gh-pages": "cross-env DEPLOY_TARGET=gh-pages next build",
    "prepare-gh-pages": "node scripts/prepare-gh-pages.js",
    "deploy:gh-pages": "npm run clean && npm run export:gh-pages && npm run prepare-gh-pages && npx gh-pages -d out",
    "deploy:cloudflare": "npm run build && npx wrangler pages deploy .next",
    "dev:cloudflare": "npx wrangler pages dev .next --port 8788",
    "prepare-netlify": "node scripts/prepare-netlify.js",
    "validate-netlify": "node scripts/validate-netlify.js",
    "init-netlify": "node scripts/init-netlify.js",
    "fix-encoding": "node scripts/fix-encoding.js",
    "fix-redirects": "node scripts/fix-redirects.js",
    "fix-spa-routing": "node scripts/fix-spa-routing.js",
    "fix-hydration": "node scripts/fix-hydration.js",
    "fix-promise-polyfill": "node scripts/fix-promise-polyfill.js",
    "direct-patching": "node scripts/direct-patching.js",
    "platform-fix": "node scripts/platform-fix.js",
    "fix-deps": "npm run platform-fix && npm install --no-optional --ignore-platform --force",
    "build:netlify": "npm run pre-build && next build && node scripts/fix-asset-paths.js",
    "fix-netlify": "npm run clean && npm run pre-build && npm run build:netlify && npm run prepare-netlify && npm run fix-encoding && npm run fix-redirects && npm run fix-spa-routing && npm run fix-hydration && npm run validate-netlify",
    "deploy:netlify": "npm run fix-netlify && npx netlify deploy --prod --dir=out",
    "deploy:netlify-manual": "npm run fix-netlify && node scripts/deploy-netlify-manual.js",
    "netlify": "npm run init-netlify && npm run deploy:netlify",
    "init-github": "node scripts/init-github.js",
    "deploy:all": "npm run clean && npm run fix-netlify && npm run deploy:netlify-manual && npm run deploy:gh-pages"
  },
